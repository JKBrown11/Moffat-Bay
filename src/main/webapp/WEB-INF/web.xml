<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>CraftmartEmployee</display-name>
  
  <!-- Set the landing page, name the server, class, and url to trigger servlet -->
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>


	<servlet>
		<servlet-name>CMServlet</servlet-name>
		<servlet-class>craftmartTools.CMServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CMServlet</servlet-name>
		<url-pattern>/addTeam.html</url-pattern>
		<!--  added the star -->
	</servlet-mapping>

</web-app>